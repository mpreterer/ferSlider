{
  "compilerOptions": {
    "outDir": "./built",
    "sourceMap": true,
    "target": "es5",
    "module": "commonjs",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strictPropertyInitialization": false,
    "experimentalDecorators": true,
    "strict": true,
    "allowJs": true,
  },
  "exclude": [
    "node_modules",
    "dist"
],
}
